{
    "profiles(permissions=***-)": {
        "_id[36]": "str<username>",
        "name[30]": "str",
        "avatar[10]": "str",
        "bio[500]": "str",
        "*userId[36]": "str",
        "lastOnline": "datetime"
    },
    "groups(permissions=**-*)": {
        "*sourceId[36]": "str",
        "*name[30]": "str",
        "logo[50]": "str",
        "isPrivate(default=false)": "bool",
        "requestIds[36]": "array",
        "dstEntities[50]": "array"
    },
    "messages(permissions=****)": {
        "*sourceId[36]": "str",
        "*text[200]": "str",
        "replyId[36]": "str",
        "video[100]": "str",
        "link": "url",
        "images[100]": "array",
        "tags[20]": "array",
        "lat": "double",
        "lng": "double",
        "isPrivate(default=false)": "bool",
        "isRead[36]": "array",
        "entitiesId[36]": "str",
        "toGroup(default=false)": "bool"
    },
    "atlas(permissions=***-)": {
        "tag[20]": "str",
        "entities[50]": "array"
    },
    "secrets(security=true)": {
        "conversations[50]": "array",
        "contacts[50]": "array"
    },
    "notifications(permissions=-*-*)": {
        "title[50]": "str",
        "body[200](default='{}')": "json(str)",
        "*sourceId[36]": "str",
        "*destinationId[50]": "str"
    }
}